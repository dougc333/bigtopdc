0	val_0
0	val_0
0	val_0
2	val_2
4	val_4
5	val_5
5	val_5
5	val_5
8	val_8
9	val_9

select * from src_multi2 order by key, value
11	val_11
12	val_12
12	val_12
15	val_15
15	val_15
17	val_17
